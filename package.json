  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "parcel": "^1.8.1",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "app-root-path": "^2.0.1",
